            <exec.executable>java</exec.executable>
        </properties>
    </action>
    <action>
        <actionName>CUSTOM-not-simple-broker</actionName>
        <displayName>not-simple-broker</displayName>

        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:1.1.1:exec</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>cismet-dev-exec</activatedProfile>
        </activatedProfiles>
        <properties>
            <jalopy.skip>true</jalopy.skip>
            <exec.format.skip>true</exec.format.skip>
