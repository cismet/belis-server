            <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=y,address=8998,suspend=n -Xmx1024m -classpath ${de.cismet.cids.classpath} Sirius.server.middleware.impls.proxy.StartProxy runtime.properties</exec.args>
            <exec.executable>java</exec.executable>
        </properties>
    </action>
    <action>
        <actionName>CUSTOM-not-simple-domainserver</actionName>
        <displayName>not-simple-belis2-domainserver</displayName>
        <goals>
            <goal>process-classes</goal>
            <goal>org.codehaus.mojo:exec-maven-plugin:1.1.1:exec</goal>
        </goals>
        <activatedProfiles>
            <activatedProfile>cismet-dev-exec</activatedProfile>
        </activatedProfiles>
        <properties>
            <jalopy.skip>true</jalopy.skip>
            <exec.format.skip>true</exec.format.skip>
            <exec.classpathScope>runtime</exec.classpathScope>
            <exec.workingdir>${de.cismet.cids.server.workingDir.belis.leo.not.simple}</exec.workingdir>
            <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=y,address=8999,suspend=n -Xmx1024m -classpath ${de.cismet.cids.classpath} Sirius.server.middleware.impls.domainserver.DomainServerImpl runtime.properties</exec.args>
            <exec.executable>java</exec.executable>
